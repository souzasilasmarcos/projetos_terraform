{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Deny",
        "Principal": "*",
        "Action": "execute-api:Invoke",
        "Resource": "arn:aws:execute-api:us-east-1:sua_account_id:ai_api/*",
        "Condition": {
          "StringNotEquals": {
            "aws:sourceVpce": "vpce-xpto"
          }
        }
      },
      {
        "Effect": "Allow",
        "Principal": "*",
        "Action": "execute-api:Invoke",
        "Resource": "arn:aws:execute-api:us-east-1:sua_account_id:id_pai/*"
      }
    ]
  }