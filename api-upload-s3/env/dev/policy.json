{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid":    "S3",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:CreateBucket",
                "s3:DeleteObject",
                "s3:PutObject"
                                
            ],
            "Resource": [
                "arn:aws:s3:::dev-xpto-upload-s3",
                "arn:aws:s3:::dev-xpto-upload-s3/*",
                "arn:aws:s3:::dev-lambda-xpto-sqs",
                "arn:aws:s3:::dev-lambda-xpto-sqs/*"
            ]            
        }        
    ]
}